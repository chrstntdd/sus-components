{
  "name": "sus-components",
  "version": "0.0.1",
  "author": "Christian Todd <christian.todd7@gmail.com> (https://www.chrstntdd.com/)",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "source": "src/index.tsx",
  "types": "dist/src",
  "sideEffects": false,
  "scripts": {
    "start": "parcel serve examples/index.html",
    "dev": "yarn start && concurrently -k -p \"[{name}]\" -n \"ExampleApp\" -c \"green.bold\" 'yarn build-lib'",
    "build-lib": "./node_modules/.bin/ts-node scripts/build.ts",
    "build-example": "parcel build examples/index.html",
    "docz-dev": "docz dev",
    "docz-build": "docz build",
    "prettier": "./node_modules/.bin/prettier --write \"src/**/*.{ts,tsx,mdx}\""
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.15",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "docz": "^0.13.4",
    "docz-theme-default": "^0.13.4",
    "husky": "^1.2.1",
    "jest": "^23.6.0",
    "jest-dom": "^3.0.0",
    "jest-watch-typeahead": "^0.2.0",
    "lint-staged": "^8.1.0",
    "parcel": "^1.11.0",
    "prettier": "^1.15.3",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-testing-library": "^5.4.0",
    "rollup": "^0.68.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-typescript": "^1.0.0",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "./node_modules/.bin/prettier --write",
      "git add"
    ]
  }
}
